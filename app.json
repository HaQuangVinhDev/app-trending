{
  "expo": {
    "name": "app-trending",
    "slug": "app-trending",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Bshop.png",
    "scheme": "myapp",
    "permissions": {
      "cameraRoll": true
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.haquangvinh.apptrending"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "35dac263-7b22-45d4-97c1-b344f12ba0f3"
      }
    },
    "owner": "haquangvinh"
  }
}